{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        },
        {
            "source": "/localhost::3000/(.*)",
            "headers": [
                {
                    "source": "/(.*)",
                    "headers": [
                        {
                            "key": "access-control-allow-origin",
                            "value": "*"
                        }
                    ]
                }
            ]
        }
    ]
}